{
    "testRegex": "/*.xtest.js$",
    "collectCoverage": false,
    "testPathIgnorePatterns":[
        "/node_modules/",
        "/umd/",
        "/esm/",
        "/Prototypes/",
        "/prototyping/",
        "/assets/",
        "/docs/",
        "/todo/"
      ],
    "coverageReporters": ["lcov"],
    "coverageDirectory": "coverage",
    "coverageThreshold": {
     "global": {
     "branches": 0,
     "functions": 0,
     "lines": 0,
     "statements": 0
     }
    },
    "moduleDirectories": ["node_modules", "src"],
    "transform": {
        "^.+\\.[t|j]sx?$": "babel-jest"
      }
   }